{
    "name": "BolinStuck",
    "description" : "Bill Bilon's music back to homestuck",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["honk.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}